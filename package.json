	"license": "BSD",
	"dependencies": {
		"body-parser": "^1.9",
	    "connect-flash": "~0.1.1",
		"cookie-parser": "^1.3.2",
		"ejs": "^1.0",
		"errorhandler": "^1.1.1",
		"express": "^4.5",
		"express-session": "^1.6.3",
		"marked": "^0.3.2",
		"morgan": "^1.4",
		"mustache": "^0.8.2",
		"semver": "^2.2",
		"serve-favicon": "^2.0.1",
		"stylus": "^0.43.1",
		"winston": "^0.7.3"
